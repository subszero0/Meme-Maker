{
  "dashboard": {
    "id": null,
    "title": "Meme Maker - Staging Overview",
    "tags": ["meme-maker", "development"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "panels": [
      {
        "id": 1,
        "title": "Service Health",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"meme-maker-backend\"}",
            "legendFormat": "Backend",
            "refId": "A"
          },
          {
            "expr": "up{job=\"redis\"}",
            "legendFormat": "Redis",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN"
                  },
                  "1": {
                    "text": "UP"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"meme-maker-backend\"}[5m])",
            "legendFormat": "{{method}} {{handler}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec"
          },
          {
            "show": false
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Queue Depth",
        "type": "graph",
        "targets": [
          {
            "expr": "redis_queue_length{queue=\"clips\"}",
            "legendFormat": "Clips Queue",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Queue Length"
          },
          {
            "show": false
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Job Processing Latency (95th percentile)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(clip_job_latency_seconds_bucket[5m]))",
            "legendFormat": "95th percentile",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds"
          },
          {
            "show": false
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "container_memory_usage_bytes{name=~\"meme-maker-.*\"} / 1024 / 1024",
            "legendFormat": "{{name}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "MB"
          },
          {
            "show": false
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{name=~\"meme-maker-.*\"}[5m]) * 100",
            "legendFormat": "{{name}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "CPU %"
          },
          {
            "show": false
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      }
    ],
    "version": 1
  },
  "folderId": null,
  "overwrite": false
} 