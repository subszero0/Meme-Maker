    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.8.3",
    "cypress": "^14.4.0",
    "cypress-real-events": "^1.14.0",
    "eslint": "^9",
