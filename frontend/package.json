    "cypress:run": "cypress run",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:smoke": "cypress run --spec 'cypress/e2e/smoke.cy.ts'",
    "visual:baseline": "percy exec -- cypress run --spec 'cypress/e2e/visual_regression.cy.ts'",
    "visual:test": "percy exec -- cypress run --spec 'cypress/e2e/visual_regression.cy.ts'"
  },
  "dependencies": {
