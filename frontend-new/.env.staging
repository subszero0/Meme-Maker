# Staging Environment Configuration for Frontend
# Used during the Vite build process for staging mode

# API Configuration - Use empty string for staging since nginx handles /api prefix
VITE_API_BASE_URL=""
VITE_API_TIMEOUT=30000

# Application Configuration
VITE_MODE=staging
VITE_APP_ENV=staging
VITE_APP_NAME="Meme Maker (Staging)"

# Development Features
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_DEBUG_MODE=true

# Performance Configuration
VITE_ENABLE_BUNDLE_ANALYZER=false
VITE_ENABLE_SOURCE_MAPS=true

# Feature Flags for Staging
VITE_ENABLE_STAGING_BANNER=true
VITE_ENABLE_DEBUG_PANEL=true 